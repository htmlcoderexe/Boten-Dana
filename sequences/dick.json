{
  "name": "dick",
  "display_name": "nenuca v2.0",
  "description": "Отращивание письки в чате, платонический идеал виртуальной фаллометрии с элементами гамификации.",
  "version": [
    1,
    0,
    0,
    0
  ],
  "triggers": [
    {
      "type": "text_exact",
      "params": ["перенести пепису","перенести пипису"],
      "subseq": "transfer"
    },
    {
      "type": "text_exact",
      "params": ["топ пепис","топ пипис"],
      "subseq": "top_dicks"
    }
  ],
  "commands": {
    "dick":
    {
      "description": "Отрастить пепису",
      "subseq": "dick_main"
    },
    "top_dicks":
    {
      "description": "Топ писек в чятике",
      "subseq": "top_dicks"
    }
  },
  "subseqs": {
    "transfer": [
      {
        "action": "get_mentions",
        "params": ["userids"],
        "target": "reply"
      },
      {
        "action": "get_numbers",
        "params": ["msgnumbers"],
        "target": "reply"
      },
      {
        "action": "index",
        "params": ["*userids",0,"user_id"]
      },
      {
        "action": "index",
        "params": ["*msgnumbers",0,"pp_size"]
      },
      {
        "action": "kill_msg",
        "params": [5]
      },
      {
        "action": "if_eq",
        "params": ["*__uid","*user_id","transfer_start","not_you"]
      }
    ],
    "not_you": [
      {
        "action": "emit_text",
        "params": ["not_you",30]
      }

    ],
    "transfer_start": [
      {
        "action": "now",
        "params": ["now"]
      },
      {
        "action": "add",
        "params": ["*now", 21600,"now"]
      },
      {
        "action": "score_set",
        "params": ["*__uid","dick","*pp_size","____",
          "*now"]
      },
      {
        "action": "escape",
        "params": ["*pp_size","pp_size"]
      },
      {
        "action": "emit_text",
        "params": ["transfer_ok",30]
      },
      {
        "action": "gosub",
        "params": ["grow_dick"]
      }
    ],
    "dick_main": [
      {
        "action": "get_time",
        "params": ["now"]
      },
      {
        "action": "add",
        "params": ["*now", 21600,"now"]
      },
      {
        "action": "score_get",
        "params": [
          -1,
          "dick",
          "dick_today",
          "day",
          "*now"
        ]
      },
      {
        "action": "kill_msg",
        "params": [5]
      },
      {
        "action": "if_eq",
        "params": [
          "*dick_today",
          0,
          "grow_dick",
          "already_played"
        ]
      }
    ],
    "grow_dick": [{
        "action": "score_get",
        "params": [
          -1,
          "dick",
          "total",
          "all",
          "*now"
        ]
      },{
        "action": "score_get",
        "params": [
          -1,
          "dick_rolls",
          "total_rolls",
          "all",
          "*now"
        ]
      },{
        "action": "score_get",
        "params": [
          -1,
          "dick_plays",
          "total_plays",
          "all",
          "*now"
        ]
      },
      {
        "action": "div",
        "params": ["*total_rolls","*total_plays","pity"]
      },
      {
        "action": "add",
        "params": ["*pity",-10.5, "pity"]
      },
      {
        "action": "mul",
        "params": ["*pity",-0.2, "pity"]
      },
      {
        "action": "if_gte",
        "params": [30,"*total","grow",""]
      },
      {
        "action": "roll_dice",
        "params": [1,20,"today_outcome"]
      },
      {
        "action": "add",
        "params": ["*today_outcome","*pity","today_outcome"]
      },
      {
        "action": "score_up",
        "params": ["*__uid","dick_plays",1,"____",
          "*now"]
      },
      {
        "action": "score_up",
        "params": ["*__uid","dick_rolls","*today_outcome","____",
          "*now"]
      },
      {
        "action": "if_gte",
        "params": [1,"*today_outcome","crit_fail",""]
      },
      {
        "action": "if_gte",
        "params": [6,"*today_outcome","shrink",""]
      },
      {
        "action": "if_gte",
        "params": [19,"*today_outcome","grow","crit_succ"]
      }
    ],
    "crit_fail": [
      {
        "action": "roll_dice",
        "params": [2,5,"today"]
      },
      {
        "action": "emit_text",
        "params": ["crit_fail",30]
      },
      {
        "action": "mul",
        "params": ["*today",-1, "today"]
      },
      {
        "action": "score_up",
        "params": ["*__uid","dick","*today","total",
          "*now"]
      }
    ],
    "shrink": [
      {
        "action": "roll_dice",
        "params": [1,5,"today"]
      },
      {
        "action": "emit_text",
        "params": ["norm_shrink",30]
      },
      {
        "action": "mul",
        "params": ["*today",-1, "today"]
      },
      {
        "action": "score_up",
        "params": ["*__uid","dick","*today","total",
          "*now"]
      }

    ],
    "grow": [
      {
        "action": "roll_dice",
        "params": [1,5,"today"]
      },
      {
        "action": "add",
        "params": ["*today",2,"today"]
      },
      {
        "action": "score_up",
        "params": ["*__uid","dick","*today","total",
          "*now"]
      },
      {
        "action": "emit_text",
        "params": ["norm_grow",30]
      }

    ],
    "crit_succ": [
      {
        "action": "roll_dice",
        "params": [2,6,"today"]
      },
      {
        "action": "score_up",
        "params": ["*__uid","dick","*today","total",
          "*now"]
      },
      {
        "action": "emit_text",
        "params": ["crit_succ",30]
      }

    ],
    "top_dicks":
            [
              {
                "action": "kill_msg",
                "params": [5]
              },
              {
                "action": "scoreboard",
                "params": ["dontcare",-1,"all",-1,20,"dick"]
              },
              {
                "action": "fmt_list",
                "params": ["dontcare","dick_board_fmt","top"]
              },
              {
                "action": "emit_text",
                "params": ["dick_board",15]
              }
            ],
    "already_played": [
      {
        "action": "emit_text",
        "params": ["already_played",30]
      }
    ]
  },
  "stringpools": {
    "already_played": ["Ти сьогоднi вже играв\\."],
    "not_you": ["Мне кажется\\, кто\\-то пытается украсть чужое хуищще\\."],
    "transfer_ok": ["Удалось перенести {pp_size} члена\\."],
    "crit_fail": [
      "Прилетела акула и отгрызла {today} см\\. Осталось всего {total} см песюна\\.",
      "Как оказалось\\, опыты с бензопилой ни к чему хорошему не приведут\\. Резултат \\- потеря уймы крови и {today} см песюна\\. Береги оставшиеся {total} см\\!"
    ],
    "crit_succ": [
      "В завещании недавно умершего Рокко Сифреди было указано передать его легендарный член именно тебе\\. \\+{today} см к письке\\. Рокко смотрит на твои {total}см с небес и улыбается\\.",
      "Применяя запретные техники якутских шаманов\\, тебе удалось растянуть член на ещё {today} см\\. Теперь он целых {total} в длину\\."
    ],
    "norm_shrink": [
      "Облом\\! Теперь песюн короче на {today} см\\. Теперь его длина {total} см\\.",
      "Похолодало \\:\\( Песюн сморщился на {today} см\\. Теперь он всего {total} см\\."
    ],
    "norm_grow": [
      "Расти\\, писька\\! {today} см добавлены\\, теперь твой меч правосудия уже {total} см\\.",
      "Эрекция наступила внезапно, плюс {today} см\\! Теперь твой монстр {total} см"
    ],
    "dick_board": [
      "```\nчлен       владелец \n{top}```"
    ],
    "dick_board_fmt": [
      "\\{0[1]:6d} см  {0[0]: <13.13}\n"
    ]
  }
}
