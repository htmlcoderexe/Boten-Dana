{
  "name": "whois",
  "display_name": "Анализ пользователя",
  "description": "Показывает информацию о пользователе.",
  "version": [1,0,0,0],
  "triggers": [
    {
      "type": "text_exact",
      "params": ["бот это кто"],
      "subseq": "main"
    },
    {
      "type": "text_exact",
      "params": ["бот кто это"],
      "subseq": "main"
    },
    {
      "type": "retval",
      "params": [],
