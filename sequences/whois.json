      {
        "action": "emit_text",
        "params": ["ok",60]
      },
      {
        "action": "kill_msg",
        "params": [0]
      }
    ],
    "404": [
      {
        "action": "emit_text",
        "params": ["404",10]
      },
      {
        "action": "kill_msg",
        "params": [0]
      }
