{
  "name": "whois",
  "display_name": "Анализ пользователя",
  "description": "Показывает информацию о пользователе.",
  "version": [1,0,0,0],
  "triggers": [
    {
      "type": "text_exact",
      "params": ["бот это кто"],
      "subseq": "main"
    },
    {
      "type": "text_exact",
      "params": ["бот кто это"],
      "subseq": "main"
    },
    {
      "type": "retval",
      "params": [],
      "subseq": "404"
    }
  ],
  "subseqs": {
    "main": [
      {
        "action": "get_uid",
        "params": ["uid"],
        "target": "reply"
      },
      {
        "action": "get_user",
        "params": ["*uid","userinfo"]
      },
      {
        "action": "obj_read",
        "params": ["userinfo","current_chat","usermeta"]
      },
      {
        "action": "qdb_get_user",
        "params": ["*uid","quote_list",4, "global", 0, "random"]
      },
      {
        "action": "fmt_list",
        "params": ["quote_list","user_quotes_format","quotes"]
      },
      {
        "action": "emit_text",
        "params": ["ok",60]
      },
      {
        "action": "kill_msg",
        "params": [0]
      }
    ],
    "404": [
      {
        "action": "emit_text",
        "params": ["404",10]
      },
      {
        "action": "kill_msg",
        "params": [0]
      }
    ],
    "self": [
      {
        "action": "emit_text",
        "params": ["self",-1]
      }
    ]
  },
  "stringpools": {
    "ok": ["Пользователь {userinfo.current_nick}\naka {userinfo.nicknames}\nid {userinfo.id}\nпепяка: {usermeta.reputation}\n%medals%\nпоследнее действие: %recent% назад\nпервое сообщение: %joindate%\n%isreturner%{quotes}\n    "],
    "404": ["Да хуй знает, в первый раз вижу"],
    "self": ["А я-то что? Я бот, отвали, противный."],
    "nomessage": ["Ты про кого? Я тут всех знаю..."],
    "user_quotes_format": [
      "\n*№{0.id}*\n\\<\\=\\=\\=\\=\\=\\=\\=\\=\\=\\=\\=\\=\\=\n{0.text_raw}\n\\=\\=\\=\\=\\=\\=\\=\\=\\=\\=\\=\\=\\=\\>"
    ]
  }
}