{
  "name": "scoreboards",
  "display_name": "Доски почёта",
  "description": "Доски почётные и не очень.",
  "version": [1,0,0,0],
  "triggers": [
    {
      "type": "text_exact",
      "params": ["доска войсоблядей"],
      "subseq": "voice_stats"
    },
    {
      "type": "text_exact",
      "params": ["твоя"],
      "subseq": "tvoya",
      "tag": "pizda_message"
    }
  ],
  "subseqs":
  {
    "main": [
      {
        "action": "check_userlist",
        "params": ["pizda_unsubscribe","exit","subscribed"]
      }
    ],
    "voice_stats": [
      {
        "action": "scoreboard",
        "params": ["voice",10,"voice_whores","board_line",60]
      }
    ],
    "pizda": [
      {
        "action": "reply_pool",
        "params": ["pizda_pool",-1,"pizda_message"]
      }
    ],
    "tvoya": [
      {
        "action": "reply_pool",
        "params": ["tvoya_pool",-1,"tvoya_message"]
      }
    ],
    "exit": []
  },
  "stringpools": {
    "voice_whores": ["Доска почёта войсоблядей\n```Войсоблядь\n{stats}\n```"],
    "board_line": ["{:^25}{:^10}"]
  }
}